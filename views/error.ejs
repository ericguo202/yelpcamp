<% layout("layouts/boilerplate") %>
    <div class="row">
        <div class="alert alert-danger col-6 offset-3" role="alert">
            <h4 class="alert-heading">
                Error <%= err.status %>: <%= err.message %>
            </h4>
            <p>
                <%= err.stack %>
            </p>
            <hr>
            <a href="/campgrounds" class="btn btn-secondary">Back</a>
        </div>
    </div>